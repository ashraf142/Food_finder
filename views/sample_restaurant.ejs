<table class="table">
    <thead class="thead-dark">
      <tr>
        <th scope="col">id</th>
        <th scope="col">Name</th>
        <th scope="col">Type</th>
        
      </tr>
    </thead>
    <tbody>
    
    <% for(var i = 0; i < items.length; i++ ) { %>
        <tr>
            <th scope="row"> <%- i %> </th>
            <td><%- items[i].name %></td>
            <td><%- items[i].type %></td>
            <td><%- items[i].id %></td>

        </tr>

    <% } %>

    </tbody>
  </table>