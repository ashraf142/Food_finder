<form type = "button" action="/insert_food" method="post">
    <div class="row">
        <div class="col-md-12">
            <div class="form-group">
                <label for="food_name">Food Name</label>
                <input type="text" class="form-control" id="food_name" name = "name" placeholder="Enter a food name." >
            </div>

            <div class="form-group">
                <label for="food_type">Food Type</label>
                <input type="text" class="form-control" id="food_type" name = "type" placeholder="Enter food type." >
            </div>
        </div>
    </div>
    <div class="row">
        <button type="submit" class="btn btn-warning">Save</button>        
    </div>
</form>